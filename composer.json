{
    "name": "scolib/attributes",
    "type": "library",
    "description": "Attributes Trait",
    "keywords": [
        "ScoLib",
        "attributes"
    ],
    "homepage": "https://github.com/ScoLib/attributes",
    "license": "MIT",
    "authors": [
        {
            "name": "klgd",
            "email": "slice1213@gmail.com",
            "homepage": "http://www.scophp.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "illuminate/support": "^5.4"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.9",
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Sco\\Attributes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sco\\Attributes\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
